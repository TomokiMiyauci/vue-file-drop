{
  "name": "vue-file-drop",
  "version": "0.2.0",
  "author": "TomokiMiyauci <development.operation.6.6.6@gmail.com>",
  "description": "Vue component for File DnD area and call File Select Dialog",
  "license": "MIT",
  "main": "dist/vue-file-drop.cjs.js",
  "module": "dist/vue-file-drop.esm-bundler.js",
  "unpkg": "dist/vue-file-drop.global.js",
  "jsdelivr": "dist/vue-file-drop.global.js",
  "sideEffects": false,
  "keywords": [
    "file",
    "drop",
    "drag",
    "DnD",
    "vue3",
    "vue",
    "library",
    "component",
    "file-dialog"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rollup -c rollup.config.ts",
    "lint:markup": "vti diagnostics",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "file-select-dialog": "^1.4.0",
    "vue": "^3.0.3"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@mdi/js": "^5.8.55",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/compiler-sfc": "^3.0.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.2",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "pascal-case": "^3.1.1",
    "prettier": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.29.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2",
    "vite": "^1.0.0-rc.13",
    "vti": "^0.0.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
